  <div class="create">
    <div class="container">
      <%= form_for(@fandom) do |f| %>  
  		  <div class="field"> 
    		  <%= f.label :fandom %><br> 
    		  <%= f.text_area :name %>
          <%= f.text_area :category %>
  		  </div> 
  		  <div class="actions"> 
    		  <%= f.submit "Create" %> 
  		  </div> 
		  <% end %>
    </div>
  </div>
</body>